---
import "../styles/style.css"
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Time tracking dashboard</title>
	</head>
	<body>
		<main class="profile">
			<div class="profile__head">sdf</div>
			<div class="profile__container"></div>
		</main>
	</body>
</html>

<style>
	.profile {
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
	}

	.profile__head {
		max-width: 375px;
		width: 100%;
	}

	.profile__container {
		max-width: 375px;
		width: 100%;
		display: grid;
		grid-template-columns: 327px;
		grid-template-rows: repeat(6, 160px);
		gap: 10px;
	}
</style>
